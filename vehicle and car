import java.util.Scanner;

class Vehicle {
    String brand;

    void showBrand() {
        System.out.println("Brand:" + brand);
    }
}

class Car extends Vehicle {
    int speed;

    void showSpeed() {
        System.out.println("Speed:" + speed);
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        Car c = new Car();

        c.brand = sc.nextLine();
        c.speed = sc.nextInt();

        c.showBrand();
        c.showSpeed();
    }
}
