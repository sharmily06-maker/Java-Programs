import java.util.Scanner;
interface Vehicle {
    void speed();
}
class Car implements Vehicle {
    public void speed() {
        System.out.println("120km/h");
    }
}
class Bike implements Vehicle {
    public void speed() {
        System.out.println("80km/h");
    }
}
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String type = sc.next();
        Vehicle v;
        switch(type) {
            case "car":
                v = new Car();
                break;
            case "bike":
                v = new Bike();
                break;
            default:
                return;
        }
        v.speed();
    }
}
