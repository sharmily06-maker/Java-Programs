import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String S=sc.nextLine();
        int start=sc.nextInt();
        int end =sc.nextInt();
        StringBuilder sb=new StringBuilder(S);
        sb.delete(start,end);
        System.out.println(sb);
    }
}
