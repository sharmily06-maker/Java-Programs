import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int count=0;
        for(int i=0;i<N;i++){
            int num=sc.nextInt();
            if(num%2 !=0)
             count ++;    
            
        }
        System.out.println(count);
    }
}
