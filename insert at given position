import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String S=sc.nextLine();
        String T=sc.nextLine();
        int n=sc.nextInt();
        StringBuilder sb=new StringBuilder(S);
        sb.insert(n,T);
        System.out.println(sb);
    }
}
    
