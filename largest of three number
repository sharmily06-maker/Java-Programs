import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int A=sc.nextInt();
        int B=sc.nextInt();
        int C=sc.nextInt();
       
        int largest;
        if(A>=B&&A>=B){
            largest=A;
        }
        else if(B>=C&&B>=A){
            largest=B;
        }
        else{
            largest=C;
        }
        System.out.println(largest);
        
    }
}
